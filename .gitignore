# =========================================
# Base Git Ignore
# =========================================

# Byte-compiled / optimized / DLL files
__pycache__/
*.py[cod]
*$py.class

# Logs
*.log
*.out
*.err

# Virtual environments
.venv/
env/
venv/

# =========================================
# macOS
# =========================================

.DS_Store
.AppleDouble
.LSOverride
Icon?
._*
.Spotlight-V100
.Trashes
.DS_Store?
.AppleDB
.AppleDesktop
Network Trash Folder
Temporary Items
.apdisk

# =========================================
# Terraform
# =========================================

# Local .terraform directories
**/.terraform/*
# Terraform lock file
.terraform.lock.hcl
# Plan output files
*.tfplan
# Local-only variables and state
*.tfstate
*.tfstate.*
crash.log
crash.*.log
override.tf
override.tf.json
*_override.tf
*_override.tf.json

# Sensitive variable files
terraform.tfvars
*.auto.tfvars

# =========================================
# Terragrunt
# =========================================

# Cache and temporary directories
**/.terragrunt-cache/
**/.terragrunt-cache/*
# Plan outputs
*.hcl*.backup
*.hcl*.tmp
# Terragrunt debug logs
terragrunt-debug.log
# Local variable overrides
*_override.hcl
# Auto-generated plan files
planfile

# =========================================
# Ansible
# =========================================

*.retry
*.vault
*.cfg
inventory/*_tmp
roles/*/files/tmp/
host_vars/*/tmp/
group_vars/*/tmp/
__ansible_tmp*

# =========================================
# Editor / IDE
# =========================================

.idea/
.vscode/
*.swp
*.swo
*.bak
*.tmp

# =========================================
# System / Misc
# =========================================

Thumbs.db
ehthumbs.db
Desktop.ini
